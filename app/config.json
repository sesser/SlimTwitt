{
	"app": {
		"routes": {
			"/": {
				"name": "home",
				"callback": [ "Pages", "home" ]
			},
			"/about": {
				"name": "about",
				"callback": [ "Pages", "about" ]
			},
			"/contact(/:status)": {
				"method": ["POST", "GET"],
				"name": "contact",
				"callback": [ "Pages", "contact" ],
				"conditions": {
					"status": "(success|error)"
				}
			}
		}
	}
}
